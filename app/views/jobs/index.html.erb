<h1>Listing jobs</h1>

<table>
  <tr>
    <th>Jobid</th>
    <th>Type</th>
    <th>Jobdesc</th>
    <th>Exp year</th>
    <th>Exp mon</th>
    <th>Salary</th>
    <th>Vacency</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobs.each do |job| %>
  <tr>
    <td><%= job.JobId %></td>
    <td><%= job.type %></td>
    <td><%= job.JobDesc %></td>
    <td><%= job.exp_year %></td>
    <td><%= job.exp_mon %></td>
    <td><%= job.salary %></td>
    <td><%= job.Vacency %></td>
    <td><%= job.Location %></td>
    <td><%= link_to 'Show applicants', job %></td>
    <td><%= link_to 'Edit', edit_job_path(job) %></td>
    <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Job', new_job_path %>
